snow
{
    cull none
    entityMergable
    nomipmaps
    nopicmip
	if ( $snowenabled != "yes" )
    {
        clampmap gfx/misc/snow.tga
        tcmod rotate 100
        blendfunc GL_ONE GL_ONE
        rgbGen        vertex
        alphaGen    vertex
    }
	if ( $snowenabled == "yes" )
	{
		map textures/qmenu/blank.tga
		detail
	}
}



