console
{
	nopicmip
	nomipmaps
	{
		map gfx/misc/console01.tga 
	}
	if ( $con_version == "old" )
	{
		map gfx/misc/console01.tga 
	}
	if ( $con_version == "new" )
	{
		map gfx/misc/console02.tga 
	}
	if ( $con_version == "red" )
	{
		map gfx/misc/console03.tga 
	}
	if ( $con_version == "jump" )
	{
		map gfx/misc/console04.tga
	}
	if ( $con_version == "simple" )
	{
		map gfx/misc/console05.tga
	}
	if ( $con_version == "town" )
	{
		map gfx/misc/console06.tga
	}
	if ( $con_version == "quak" )
	{
		map gfx/misc/console07.tga
	}
	if ( $con_version == "black" )
	{
		map $whiteimage
        rgbgen const ( 0 0 0 )
	}
}

